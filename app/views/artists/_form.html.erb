<% if flash[:form_errors] %>
<ul>
    <% flash[:form_errors].each do |error| %>
        <li><%=error%></li>
    <% end %>
</ul>
<% end %>

<%= form_for @artist do |f| %>
    <%= f.label :name %>
    <%= f.text_field :name %><br>

    <%= f.label :age %>
    <%= f.text_field :age %><br>
    
    <%= f.label :title %>
    <%= f.text_field :title %><br>

    <%= f.submit %>
<% end %>

